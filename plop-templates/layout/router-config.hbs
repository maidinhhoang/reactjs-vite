{
  path: ELayoutPath.{{constantCase rawLayoutName}},
  element: {
    component: {{pascalCase layoutName}},
    isPrivate: {{lowerCase isPrivate}},
    fallbackPermissionDenied: AppPermissionDenied
  },
  children: [
    {
      element: {
        index: true,
        component: {{pascalCase layoutName}}Default,
        errorComponent: {{pascalCase layoutName}}Error
      }
    },
    {
      path: ESpecialPath.REST,
      element: {
        component: {{pascalCase layoutName}}NotFound,
        errorComponent: {{pascalCase layoutName}}Error
      }
    }
  ]
}